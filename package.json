{
  "name": "figma-plugins-stats",
  "version": "0.0.26-alpha.0",
  "description": "A CLI to get live and historical stats for your Figma plugins",
  "keywords": [
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins-stats.git"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "figma-plugins-stats": "lib/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf '*.log' build data lib",
    "fix": "eslint --fix '{scripts,src,test}/**/*.ts'",
    "lint": "eslint '{scripts,src,test}/**/*.ts'",
    "prepublishOnly": "npm run build",
    "reset": "npm run clean && rimraf node_modules package-lock.json && npm install",
    "scrape": "node --loader ts-node/esm scripts/scrape.ts",
    "start": "node --loader ts-node/esm src/cli.ts",
    "test": "ava --serial 'test/**/*.ts'"
  },
  "dependencies": {
    "@yuanqing/cli": "^0.0.9",
    "date-fns": "^2.28.0",
    "date-time": "^4.0.0",
    "fs-extra": "^10.1.0",
    "indent-string": "^5.0.0",
    "kleur": "^4.1.4",
    "ms": "^2.1.3",
    "node-fetch": "^3.2.5",
    "ora": "^6.1.0",
    "sparkly": "^6.0.0",
    "strip-ansi": "^7.0.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.40",
    "@types/node-fetch": "^2.6.1",
    "@types/text-table": "^0.2.2",
    "ava": "^4.3.0",
    "eslint": "^8.17.0",
    "eslint-config-yuanqing": "^0.0.6",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "failFast": true,
    "nodeArguments": [
      "--loader",
      "ts-node/esm"
    ],
    "timeout": "2m",
    "workerThreads": false
  },
  "eslintConfig": {
    "extends": "eslint-config-yuanqing"
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "prettier": "eslint-config-yuanqing/prettier",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run lint && npm run fix && npm run test"
  }
}
